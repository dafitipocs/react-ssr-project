{
  "name": "site-server-side-render-web",
  "version": "1.0.0",
  "description": "Site Server Side Render Web",
  "private": true,
  "author": "Newton Delbuque GuimarÃ£es",
  "main": "index.js",
  "keywords": [
    "server side render",
    "nodejs",
    "reactjs"
  ],
  "license": "ISC",
  "scripts": {
    "babel:dev": "env-cmd .env babel src --watch -d dist",
    "babel:prod": "babel src -d dist",
    "nodemon": "env-cmd .env nodemon dist/server.js",
    "webpack:dev": "env-cmd .env webpack -d -w",
    "webpack:prod": "webpack -p",
    "lint:scss": "sass-lint --config .sass-lint.yml --verbose --no-exit",
    "lint:js": "eslint --format compact src/",
    "start": "set NODE_ENV=development && npm-run-all --parallel babel:dev webpack:dev nodemon",
    "build:prod": "npm run babel:prod && npm run webpack:prod",
    "test": "mocha --reporter spec --ui bdd --require babel-register --recursive test/",
    "test:watch": "mocha --reporter spec --ui bdd --require babel-register --watch --recursive test/",
    "coverage": "nyc npm test",
    "coverage:report": "nyc --reporter lcovonly npm run test",
    "coverage:validate": "nyc check-coverage --statements 50 --lines 50 --functions 50 --branches 50",
    "coverage:report:send": "cat ./coverage/lcov.info | codacy-coverage"
  },
  "engines": {
    "npm": ">=6.2.0",
    "node": ">=10.9.0"
  },
  "jest": {
    "setupFiles": [
      "test/utils/prop-types-error.js"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-slick": "^0.25.2",
    "reactstrap": "^8.0.1",
    "redux": "^4.0.4",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.0",
    "chai": "4.1.2",
    "codacy-coverage": "3.0.0",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "env-cmd": "8.0.2",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-no-unsanitized": "3.0.2",
    "eslint-plugin-no-wildcard-postmessage": "0.1.3",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "jsdom": "^12.0.0",
    "mocha": "5.2.0",
    "node-sass": "^4.9.2",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "nyc": "13.0.1",
    "postcss": "8.4.31",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "5.3.0",
    "sass-lint": "1.12.1",
    "sass-loader": "7.1.0",
    "sinon": "6.1.5",
    "webpack": "4.17.0",
    "webpack-cli": "3.1.0"
  }
}
